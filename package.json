{
  "name": "clean-api-ts",
  "version": "0.0.1",
  "description": "Typescript API using TDD, Clean Architecture, Design Patterns and SOLID",
  "author": "@ahsouza An√≠bal Henrique De Souza <annibalhsouza@gmail.com>",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --no-cache --runInBand",
    "test:verbose": "jest --passWithNoTests --no-cache",
    "test:unit": "npm test -- --watch --no-cache -c jest-unit-config.js",
    "test:integration": "npm test -- --watch --no-cache -c jest-integration-config.js",
    "test:staged": "npm test",
    "test:ci": "npm test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahsouza/clean-api-ts.git"
  },
  "keywords": [
    "node",
    "typescript",
    "eslint",
    "jest",
    "husky",
    "tdd",
    "ddd",
    "clean-architecture",
    "solid"
  ],
  "license": "GNU",
  "bugs": {
    "url": "https://github.com/ahsouza/clean-api-ts/issues"
  },
  "homepage": "https://github.com/ahsouza/clean-api-ts#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "eslint": "^7.21.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-plugin-import": "^2.22.1",
